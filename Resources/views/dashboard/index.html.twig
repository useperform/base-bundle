{% extends '@PerformBase/base.html.twig' %}

{% block workspace %}
  <h3><i class="fa fa-angle-right"></i> Dashboard</h3>
  <div class="row dashboard">
    <div class="col-lg-8">
      {% for panel in panels.left %}
        {{ panel.render()|raw }}
      {% endfor %}
    </div>

    <div class="col-lg-4">
      {% for panel in panels.right %}
        {{ panel.render()|raw }}
      {% endfor %}
    </div>
  </div>
{% endblock %}
